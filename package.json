{
  "name": "my-humble-tests",
  "version": "7.0.0",
  "description": "helper for text highlighting",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "prepublishOnly": "yarn test && yarn lint && rm -rf dist && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimkadenisov/autorelease-test.git"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "keywords": [
    "highlighter",
    "highlight",
    "text",
    "string",
    "substring",
    "words",
    "match",
    "matches",
    "occurrences",
    "search"
  ],
  "author": "Dima Denisov",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "29.1.2",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "8.26.0",
    "husky": "8.0.0",
    "jest": "29.2.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "semantic-release": "20.1.0",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  }
}
